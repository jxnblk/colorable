    "color": "^0.7.3",
    "lodash": "^3.1.0"
  },
  "devDependencies": {
    "basscss": "^7.0.0-beta.2",
    "classnames": "^2.1.2",
    "colors.css": "^2.3.0",
    "cssnext": "^1.6.0",
    "json-loader": "^0.5.2",
    "jsx-loader": "^0.13.2",
    "marked": "^0.3.3",
    "node-libs-browser": "^0.5.2",
    "query-string": "^2.3.0",
    "react": "^0.13.3",
    "react-hsl-sliders": "^2.0.0",
    "react-router": "^0.13.3",
    "static-site-generator-webpack-plugin": "^1.0.0",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.9.0"
  },
  "directories": {
